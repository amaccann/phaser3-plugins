{
  "name": "phaser3-drag-select",
  "version": "1.1.0",
  "description": "Phaser3 Mouse Drag Select plugin",
  "main": "dist/phaser3-drag-select-plugin.js",
  "repository": "git@github.com:amaccann/phaser3-drag-select.git",
  "author": "Andrew MacCann <andrew.maccann@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack/prod.js ",
    "lint": "prettier --write ./src/**/*.js",
    "start": "webpack-dev-server --config webpack/base.js --open",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "test"
      ]
    }
  },
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "phaser": "^3.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/plugin-proposal-optional-chaining": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.10",
    "jsdoc-to-markdown": "^5.0.3",
    "lint-staged": "^10.0.1",
    "npm-watch": "^0.6.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "raw-loader": "^4.0.0",
    "terser-webpack-plugin": "^2.3.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  }
}
